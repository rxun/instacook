{
  "name": "flask-react-starter",
  "version": "0.0.1",
  "description": "Flask + React Starter",
  "main": "index.js",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "1.0.0",
    "style-loader": "0.22.1",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.2",
    "html-webpack-plugin": "^3.2.0",
    "compression-webpack-plugin": "2.0.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5"
  },
  "scripts": {
    "_clean-dist": "rm -rf dist/**",
    "_build-dist": "webpack --mode production && mkdir -p dist/static && cp -R frontend/static/* dist/static",
    "prestart": "npm run _clean-dist && npm run _build-dist",
    "start": "bash .start.sh"
  },
